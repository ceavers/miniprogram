.register_box{
  position: relative;
  height: 100%;
  width: 100%;
  overflow: hidden;
}
.phone_box{
  width: 600rpx;
  height: 88rpx;
  margin: auto;
  display: flex;
  margin-top: 34rpx;
  overflow: hidden;
  background:rgba(255,255,255,1);
  border-radius:8px;
}
.phone_box image{
  width:60rpx;
  height: 60rpx;
  border-radius: 50%;
  box-shadow:1px 4px 7px 0px rgba(98,172,75,0.43);
  margin-top: 14rpx;
  margin-left: 30rpx;
}
.phone_box input{
  height: 100%;
  width: auto;
  margin-left: 22rpx;
  font-size: 28rpx;
  font-weight:500;
}
.input_public{
  width: 600rpx;
  height: 88rpx;
  background:rgba(255,255,255,1);
  border-radius:4px;
  margin: auto;
  display: flex;
  overflow: hidden;
  margin-top: 144rpx;
}
.input_public input{
  height: 100%;
  width: 503rpx;
   font-size: 28rpx;
   margin-left: 14rpx;
}
.input_public image{
  width: 72rpx;
  height: 72rpx;
  margin-top: 12rpx;
  margin-left: 25rpx;
}
.code_box{
  width: 600rpx;
  height: 88rpx;
  display: flex;
  justify-content: space-between;
  margin: auto;
  margin-top: 30rpx;
}
.code_input{
  width: 360rpx;
  height: 100%;
  line-height: 88rpx;
  background-color: rgba(255,255,255,1);
  border-radius:8rpx;
  box-sizing: border-box;
  padding-left: 30rpx;
}
.code_box button{
  width: 200rpx;
  height: 100%;
  background-color: rgba(98,172,75,1);
  border-radius:8rpx;
  font-size: 26rpx;
  color:rgba(255,255,255,1);
  font-weight:500;
  line-height: 88rpx;
}
.login_btn{
  width: 630rpx;
  height: 90rpx;
  margin: auto;
  background-color: rgba(67,174,58,1);
  border-radius:45rpx;
  font-size: 32rpx;
  margin-top: 48rpx;
}
.login_btn button{
 background:linear-gradient(120deg,rgba(116,189,76,1),rgba(98,172,75,1));
 border-radius:45rpx;
 color: white;
 letter-spacing:20rpx;
 font-weight:500;
}
.login_bottom{
  position: absolute;
  width: 241rpx;
  bottom: 52rpx;
  left: 255rpx;
  font-size: 22rpx;
  white-space: nowrap;
  color: #999999;
}
.login_bottom text{
  color: #323232;
}
.shodw{
  position: absolute;
}
.dialog{
  width: 550rpx;
  height: 800rpx;
  background-color: rgba(246,246,246,1);
  border-radius:15rpx;
  position: absolute;
  top: 0;
  bottom: 0;
  right: 0;
  left: 0;
  margin: auto;
  z-index: 100;
}
.gou{
  height: 120rpx;
  width: 120rpx;
  background:linear-gradient(120deg,rgba(116,189,76,1),rgba(98,172,75,1));
  border-radius:50%;
  line-height: 120rpx;
  text-align: center;
  color: white;
  font-size: 58rpx;
  font-weight: 1000;
  overflow: hidden;
  margin: auto;
  margin-top: 90rpx;
}
.dialog_title{
  text-align: center;
  margin-top: 60rpx;
  font-size:28rpx;
  font-weight:bold;
  color:rgba(50,50,50,1);
}
.dialog_text{
  font-size: 24rpx;
  color: rgba(153,153,153,1);
  font-weight:500;
  margin-top: 19rpx;
  text-align: center;
}
.dialog_pwd{
   text-align: center;
   font-size:28rpx;
  font-weight:500;
  color:rgba(50,50,50,1);
  margin-top: 50rpx;
}
.pwd{
  padding-left: 10rpx;
}
.dialog_btn{
  width:93px;
  height:23px;
  text-align: center;
  margin: auto;
  margin-top: 29rpx;
  font-size:24rpx;
  font-weight:500;
  color:rgba(98,172,75,1);
}
.login_btn{
  width:500rpx;
  height:90rpx;
  background:linear-gradient(120deg,rgba(116,189,76,1),rgba(98,172,75,1));
  border-radius:45rpx;
  margin: auto;
  text-align: center;
  margin-top: 132rpx;
  font-size:32rpx;
  font-weight:500;
  color:rgba(255,255,255,1);
  line-height: 90rpx;
}
.shodw{
  width: 100%;
  height: 100%;
  position: absolute;
  top: 0;
  background-color: black;
  opacity:0.4;
}