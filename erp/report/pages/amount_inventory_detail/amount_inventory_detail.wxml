<!--report/pages/amount_inventory_detail/amount_inventory_detail.wxml-->
<view class="container">
  <view class="title">入库明细</view>
  <view class="item-box">
    <view class="item-text">日期</view>
    <view class="item-content">{{detail.date_quarter}}</view>
  </view>
  <view class="item-box">
    <view class="item-text">入库数量</view>
    <view class="item-content">{{detail.outbound_num}}</view>
  </view>
  <view class="item-box">
    <view class="item-text">入库金额</view>
    <view class="item-content">{{detail.outbound_gross}}</view>
  </view>
</view>
