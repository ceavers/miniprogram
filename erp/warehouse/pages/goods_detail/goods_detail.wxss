.header{
  padding:0 30rpx;
  display: flex;
  justify-content: space-between;
  margin:50rpx 0 20rpx 0;
}
.text{
  font-size:28rpx;
  font-weight:500;
  color:rgba(50,50,50,1);
}
.icon-return{
  margin-left: 40rpx;
