/* warehouse/pages/storages/storages.wxss */
.title{
  font-size:32rpx;
  font-weight:bold;
  color:rgba(50,50,50,1);
  text-align: center;
  height: 88rpx;
  line-height: 94rpx;
  margin-top:14rpx;
}
.res-box{
  height: calc(100% - 190rpx);
}
.add-btn{
  position: fixed;
  display: flex;
  justify-content: center;
  align-items: center;
  width: 100%;
  bottom:0;
  height: 88rpx;
  background: #fff;
  font-size:28rpx;
  font-weight:500;
  color:rgba(66,66,66,1);
}
.add{
  width: 36rpx;
  height: 36rpx;
  margin-right: 20rpx;
}
.res-item-box{
  height: 148rpx;
  margin-bottom: 20rpx;
  background: #fff;
  padding:0 30rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
}
.icon-return{
  color:#D9D9D9;
}
.img-box{
  display: flex;
  align-items: center;
}
.img{
  width: 104rpx;
  height: 100rpx;
  margin-right: 20rpx;
}
.name{
  font-size:26rpx;
  font-weight:500;
  color:rgba(50,50,50,1);
}
.number{
  font-size:24rpx;
  font-weight:500;
  color:rgba(50,50,50,1);
  display: block;
}
.str-n{
  margin:6rpx 0;
}