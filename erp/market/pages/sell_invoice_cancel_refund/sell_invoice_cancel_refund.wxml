<view class="head">订单收款</view>
<view class="returnedInformation">
  <text class="refundMoney">退货单金额</text>
  <text>{{orderMoney}}</text>
</view>
<view class="returnedInformation">
  <text class="phone">已退金额</text>
  <text>{{refund_money}}</text>
</view>
<view class="returnInformation">
  <text class="phone">退款金额</text>
  <text>{{orderMoney}}</text>
</view>
<view class="otherInformation1">
  <view>
    <text class="wuliu">退款账号</text>
    <text>{{account_name}}</text>
  </view>
  <image class="icon-go" src="../../images/jiantou.png"></image>
</view>
<view class="otherInformation1">
  <view>
    <text class="wuliu">退款时间</text>
    <text>{{sttime}}</text>
  </view>
  <image class="icon-go" src="../../images/jiantou.png"></image>
</view>
<view class="remarkInformation">
  <text class="remark">备注</text>
  <text>大客户，单子多</text>
</view>
<view><button class="button" bindtap="button">确认退款</button></view>