/**index.wxss**/
page{
  background: #F5F5F5;
  height: 100%;
}
.hr{
  height: 1px;
  background: #F6F6F6;
}
.accredit{
  width:100%;
  height:100%;
  position: absolute;
  z-index: 999999;
  background:rgba(127, 126, 127,0.8);
}
.accredit-box{
  width: 670rpx;
  height: 518rpx;
  z-index: 9999999;
  background: #FEFCFC;
  border-radius: 10rpx;
  position: absolute;
  left:50%;
  margin-left:-335rpx;
  top:50%;
  margin-top:-259rpx;
}
.accredit-image{
  width: 576rpx;
  height: 268rpx;
  margin-left:47rpx;
}
.accredit-image2{
  position: absolute;
  width: 180rpx;
  height: 180rpx;
  top:110rpx;
  left:50%;
  margin-left:-90rpx;
}
.accredit-text{
  font-size:34rpx;
  font-weight:500;
  color:rgba(50,50,50,1);
  padding: 50rpx 0;
  text-align: center;
}
.accredit-btn{
  font-size:34rpx;
  font-weight:500;
  color:rgba(255,96,34,1);
  background: transparent;
}
.accredit-btn::after{ 
  border: none; 
}
.pup-title{
  font-size:30rpx;
  font-weight:bold;
  color:rgba(50,50,50,1);
  position: relative;
}
.pup-box{
  text-align: center;
}
.pup-image{
  width: 302rpx;
  height: 208rpx;
  margin-top:36rpx;
}
/*底部按钮*/
.modal-footer{
  display: flex;
  flex-direction: row;
  height: 80rpx;
  line-height: 80rpx;
}
.confirm-btn {
  flex: 1;
  height: 100rpx;
  line-height: 100rpx;
  text-align: center;
  font-size:28rpx;
  font-weight:500;
  color:rgba(255,96,34,1);
}
.apply-item{
  width: 100%;
  height: 88rpx;
  background:#F6F6F6;
  border-radius: 8rpx;
  margin-top:30rpx;
  display: flex;
  align-items: center;
  flex-wrap: nowrap;
}
.apply-image{
  height: 74rpx;
  width:74rpx;
  margin:0 16rpx 0 26rpx;
}
.closeBtn{
  width: 30rpx;
  height: 30rpx;
  position: absolute;
  right:0;
  top:4rpx;
}
.weui-input{
  text-align: left;
  height: 100%;
  width: 80%;
  font-size:28rpx;
}
/* 轮播图 */
.swiper{
  padding: 20rpx 0;
  height: 360rpx;
  box-sizing: border-box;
  overflow: hidden;
  background: rgba(255,255,255,1);
}
.li{
  width: 100%;
  height: 320rpx;
}
.li1{
  width: 650rpx;
  margin: 0 auto;
  border-radius: 8rpx;
  height: 320rpx;
  transform: scale(0.92);
}
swiper-item{
  width: 610rpx!important;
}
.bannerImg{
  width: 610rpx;
  height: 320rpx;
  border-radius: 8rpx;
  transform: scale(0.95);
}
.selected{
  transform: scale(1);
}
/* br分割线 */
.indexBr{
  box-sizing: border-box;
  height: 20rpx;
  background: #F6F6F6FF;
}
.bottomBr{
  box-sizing: border-box;
  height: 33rpx;
  background: #F6F6F6FF;
}
/* 新生堂简介 */
.hospDesc{
  width: 100%;
  height: 429rpx;
  box-sizing: border-box;
  margin-top:20rpx;
  background: #fff;
  overflow:hidden;

}
.hostTitle{
  padding: 12rpx 0 11rpx 30rpx;
  display: flex;
  background: #fff;
}
.hostTitle .bg{
  width: 50rpx;
  height: 50rpx;
  background-image:url('data:image/png;base64,');
  background-repeat: no-repeat;
  background-position: center;
  background-size: cover;
}
.hostTitle .desc{
  font-size: 28rpx;
  line-height: 50rpx;
  margin-left: 13rpx;
  font-weight: 500;
  color: rgba(50,50,50,1);
}
.hospBr{
  width: 100%;
  height: 1rpx;
  background-color: rgba(246,246,246,1);
}
.hospModule{
  width: 100%;
  height: 355rpx;
  box-sizing: border-box;
  padding: 0 30rpx 0 30rpx;
  display: flex;
}
.hospModule>view{
  width: 50%;
  box-sizing: border-box;
}
/* 关于医院 */
.about{
  padding: 20rpx 15rpx 20rpx 0;
  border-right: 1rpx solid rgba(246,246,246,1);
}
.aboutHosp{
  font-size: 24rpx;
  font-weight: 500;
  color: rgba(50,50,50,1);
  margin-bottom: 16rpx;
}
.aboutDesc{
  font-size: 22rpx;
  height: 88rpx;
  font-weight: 500;
  color: rgba(153,153,153,1);
  margin-bottom: 18rpx;
}
.aboutBg{
  width:314rpx;
  height: 170rpx;
}
/* 一站式孕育服务 */
.serviceDesc{
  border-bottom: 1rpx solid rgba(246,246,246,1);
  height: 52%;
  box-sizing: border-box;
  padding: 20rpx 0 20rpx 25rpx;
}
.science{
  display: flex;
}
.science>view{
  width: 50%;
}
.science :nth-child(1){
  color: rgba(153,153,153,1);
  font-size: 22rpx;
  font-weight: 500;  
}
.serviceBg{
  width: 130rpx;
  height: 95rpx;
  margin-left: 30rpx;
}
/* 客服服务 */
.accessChannel{
  padding: 12rpx 0 20rpx 30rpx;
  width: 100%;
  height: 244rpx;
  box-sizing: border-box;
  background: rgba(255,255,255,1);
  margin-top:20rpx;
}
.accessChannel .bg{
  background-image:url('data:image/png;base64,');
  background-repeat: no-repeat;
  background-position: center;
  background-size: contain;
}
.customer{
  display: flex;
  margin-top: 21rpx;
}
.customer>view{
  width: 330rpx;
  height: 130rpx;
  box-sizing: border-box;
  padding: 20rpx 0 20rpx 30rpx;
}
.onlineService{
  background: rgba(254,182,46,0.3);
  border-radius: 6rpx;
}
.vipService{
  margin-left: 30rpx;
  background: rgba(255,96,34,0.3);
  border-radius: 6rpx;
}
/* 问诊头像和描述 */
.onlineService, .vipService{
  display: flex;
}
.head{
  width: 90rpx;
  height: 90rpx;
  border-radius: 90rpx;
  background: rgba(254,182,46,1);
  display: flex;
  justify-content: center;
  align-items: center;     
}
.onlineheadBg, .vipBg{
  width: 60rpx;
  height: 60rpx;
}
.onlineheadBg{
  background-image:url('data:image/png;base64,');
  background-repeat: no-repeat;
  background-position: center;
  background-size: contain;
}
.vipBg{
  background-image:url('data:image/png;base64,');
  background-repeat: no-repeat;
  background-position: center;
  background-size: contain;
}
.customer .desc{
  margin-left: 19rpx;
}
.customer .desc :nth-child(1){
  font-size: 30rpx;
  font-weight: 500;
  color:rgba(50,50,50,1);
}
.customer .desc :nth-child(2){
  margin-top: 15rpx;
  font-size: 24rpx;
  font-weight: 500;
}
