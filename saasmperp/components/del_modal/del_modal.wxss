.del-modal{
  position: fixed;
  bottom: 50%;
  left: 50%;
  transform: translate(-50%,50%);
  z-index: 999999;
  width: 520rpx;
  border-radius: 16rpx;
  padding: 30rpx 40rpx;
  box-sizing: border-box;
  background: #fff;

}

.del-modal-title{
  text-align: center;
  color: #323232;
  margin-bottom: 30rpx;
  font-size: 32rpx;
}

.del-modal-content{
  font-size: 28rpx;
  color: #323232;
  line-height: 30rpx;
  margin-bottom: 40rpx;

} 

.del-modal-footer{
  display: flex;
  align-items: center;
  justify-content: space-between;
  font-size: 28rpx;
  color: #62AC4B;
}

.del-modal-mask{
  position: fixed;
  z-index: 99999;
  background: rgba(0,0,0,.4);
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
}