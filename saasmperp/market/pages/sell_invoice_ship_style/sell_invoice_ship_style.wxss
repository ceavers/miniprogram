.chooseStyleModel{
  width: 100%;
  height: 100%;
  background: rgba(0, 0, 0, 0.4);
  position: absolute;
  top: 0;
  left: 0;
  z-index: 99;
}
.chooseStyleModel radio-group{
  margin-top: 312rpx;
  margin-left: 125rpx;
}
.style-content{
  display: flex;
  flex: row;
  justify-content: space-between;
  align-items: center;
  height: 90rpx;
  width: 416rpx;
  padding-left: 44rpx;
  padding-right: 40rpx;
  background: #444444;
  font-size: 28rpx;
  font-weight: 500;
  color: #fff;
  border-bottom: solid 2rpx #999;
}

radio{
  border-radius: 50%;
  width: 32rpx;
  height: 32rpx;
  border: 4rpx solid #62AC4B;/* 设置边框（外圆） */
  font-size: 0;/* 让边框重合 */
}
radio .wx-radio-input{
   border-radius: 50%;
   width: 20rpx;
   height: 20rpx; 
   margin-top: 6rpx;
   margin-left: 6rpx;
   border: none;/* 替换边框（隐藏原有边框） */
   background:#444444; 
}
radio .wx-radio-input.wx-radio-input-checked::before{
  font-size: 0rpx;
  border-radius: 50%;
  width: 20rpx;
  height: 20rpx;
}

