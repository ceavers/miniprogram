  margin-left: 40rpx;
}
.public_box image{
  width: 120rpx;
  height: 120rpx;
}
.new_start{
  display: flex;
}
.start_text{
  overflow: hidden
}
.public_box_text{
  margin-left: 20rpx;
}
.first_text{
  margin-top: 54rpx;
  font-size:32rpx;
  font-weight:500;
  color:rgba(50,50,50,1);
  height: 30rpx;
}
.second_text{
  margin-top: 34rpx;
