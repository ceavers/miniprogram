.start_title{
  margin-top: 80rpx;
}
.start_title{
  overflow: hidden;
  height: auto;
}
.start_img{
  width: 160rpx;
  height: 160rpx;
  background:rgba(217,217,217,1);
  box-shadow:0px 3px 10px 0px rgba(51,51,51,0.2);
  border-radius:50%;
  margin: auto;
}
.start_img image{
   width: 160rpx;
  height: 160rpx;
}
.start_title_text{
  text-align: center;
  margin-top: 30rpx;
}
.new_start{
  width: 640rpx;
  height: 200rpx;
  background:rgba(255,255,255,1);
  box-shadow:0px 2px 4px 0px rgba(217,217,217,0.06);
  border-radius:16px;
  margin: auto;
  margin-top: 120rpx;
  overflow: hidden;
}
.public_box{
  width: 120rpx;
  height: 120rpx;
  background:rgba(217,217,217,1);
  box-shadow:0px 3px 10px 0px rgba(153,153,153,0.2);
  border-radius:50%;
