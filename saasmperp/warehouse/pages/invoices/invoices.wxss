/* warehouse/pages/invoices/invoices.wxss */
.title{
  font-size:32rpx;
  font-weight:500;
  color:rgba(50,50,50,1);
  text-align: center;
  margin:50rpx 0 40rpx 0;
}
.item{
  height: 88rpx;
  border-bottom:1px solid #f6f6f6;
  display: flex;
  padding: 0 30rpx;
  justify-content: space-between;
  align-items: center;
  background: #fff;
}
.icon-return{
  color: #D9D9D9;
  font-size: 36rpx;
}
.item-text{
  font-size:28rpx;
  font-weight:500;
  color:rgba(50,50,50,1);
}
.text-box{
  display: flex;
  align-items: center;
}
.circle{
  width: 16rpx;
  height: 16rpx;
  border-radius: 50%;
  margin:0 30rpx 0 10rpx;
}
.red{
  background: #FF3B30;
}
.green{
  background: #62AC4B;
}
.yellow{
  background: #FDD04F;
}
.blue{
  background: #62AFFC;
}